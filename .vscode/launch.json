	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch via NPM",
			"request": "launch",
			"runtimeArgs": [
				"run-script",
				"build"
			],
			"runtimeExecutable": "npm",
			"skipFiles": [
				"<node_internals>/**"
			],
			"type": "pwa-node",
			"cwd": "${workspaceFolder}/packages/source-map-test"
		},
		{
			"name": "Launch Program",
			"program": "${workspaceFolder}/packages/source-map-test/node_modules/.bin/webpack-cli",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"type": "pwa-node",
			"cwd": "${workspaceFolder}/packages/source-map-test"
		},
	]
}